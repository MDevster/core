{
  "name":"oveleon/contao-immo-manager-bundle",
  "type":"contao-bundle",
  "description":"Manage real estate properties in Cotnao.",
  "keywords":["contao","immo-manager","immo","real-estate","real estate","estate","property"],
  "homepage":"https://ovealeon.de/",
  "license":"AGPLv3",
  "authors":[
    {
      "name":"Oveleon",
      "homepage":"https://ovealeon.de/",
      "role":"Developer"
    }
  ],
  "require":{
    "php":">=7.1",
    "contao/core-bundle":"^4.4",
    "menatwork/contao-multicolumnwizard-bundle": "^3.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/core-bundle": "4.4.1",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload":{
    "psr-4": {
      "Oveleon\\ContaoImmoManagerBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "contao-manager-plugin": "Oveleon\\ContaoImmoManagerBundle\\ContaoManager\\Plugin"
  }
}