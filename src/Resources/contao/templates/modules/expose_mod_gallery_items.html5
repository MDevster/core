<?php foreach ($this->slides as $slide): ?>
    <div class="gallery-slide <?=$slide->isImage ? 'image' : 'html'?>">
        <?php if($slide->isImage): ?>
            <figure><?php $this->insert('picture_default', $slide->picture); ?></figure>
        <?php else: ?>
            <?=$slide?>
        <?php endif; ?>
    </div>
<?php endforeach; ?>